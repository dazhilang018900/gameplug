#下边这行指定生成驱动名字TpDriver.sys
TARGETNAME=TpDriver
#下边这行指定生成文件的类型DRIVER指驱动
TARGETTYPE=DRIVER
#下边这行指定生成驱动所在的路径\SYS\TpDriver.sys
TARGETPATH=SYS
#下边这行指定相关头文件所在目录路径
INCLUDES=$(BASEDIR)\inc;\
      $(BASEDIR)\inc\ddk;\

##上边必空一行E:\WinDDK\7600.16385.1 等价$(BASEDIR)
#下边这行指定驱动源代码*.cpp或者*.c
SOURCES=MyDriverSys.cpp\ 